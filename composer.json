{
  "name": "dotkernel/dot-cache",
  "description": "DotKernel cache component extending zend-cache",
  "authors": [
    {
      "name": "DotKernel Team",
      "email": "team@dotkernel.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cache",
    "services"
  ],
  "require": {
    "php": "^7.1",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-cache": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Cache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Cache\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.2-dev"
    }
  }
}
