{
    "name": "dotkernel/dot-cache",
    "description": "DotKernel cache component extending laminas-cache",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cache",
        "services"
    ],
    "require": {
        "php": "~7.4.0 || ~8.0.0 || ~8.1.0",
        "laminas/laminas-servicemanager": "^3.10",
        "laminas/laminas-cache-storage-adapter-filesystem": "^2.3",
        "laminas/laminas-cache-storage-adapter-session": "^2.4",
        "laminas/laminas-cache-storage-adapter-memory": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Cache\\": "tests"
        }
    }
}
